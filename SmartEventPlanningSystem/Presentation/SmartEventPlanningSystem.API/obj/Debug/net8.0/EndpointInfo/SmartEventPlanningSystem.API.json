          "areasOfInterest": {
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int32"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UserRegisterDto": {
        "type": "object",
        "properties": {
